<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "Server.SavingDataGame" table = "grainfo">

        <meta attribute = "class-description">
            This class contains the moves detail.
        </meta>

        <id name = "ID" type = "int" column = "ID">
            <generator class="native"/>
        </id>
        <property name = "NrGry" column = "NrGry" type = "int"/>
        <property name = "NrRuchu" column = "NrRuchu" type = "int"/>
        <property name = "info" column = "Info" type = "string"/>


    </class>
</hibernate-mapping>